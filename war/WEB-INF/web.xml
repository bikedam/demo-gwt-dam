<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>org.bikesterdam.demoDam.DemoDam DemoDamService</servlet-name>
        <servlet-class>org.bikesterdam.demoDam.server.DemoDamServiceImpl</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.bikesterdam.stockPriceWatch.StockPriceWatch StockPriceWatchService</servlet-name>
        <servlet-class>org.bikesterdam.stockPriceWatch.server.StockPriceWatchServiceImpl</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>org.bikesterdam.demoDam.DemoDam DemoDamService</servlet-name>
        <url-pattern>/DemoDam/DemoDamService</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.bikesterdam.stockPriceWatch.StockPriceWatch StockPriceWatchService</servlet-name>
        <url-pattern>/org.bikesterdam.stockPriceWatch.StockPriceWatch/StockPriceWatchService</url-pattern>
    </servlet-mapping>
</web-app>